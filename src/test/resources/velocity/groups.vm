<?xml version="1.0"?>
<ocs>
  <meta>
    <status>${status}</status>
    <statuscode>${statuscode}</statuscode>
#if ( ${message} )
    <message>${message}</message>
#else
    <message/>
#end
  </meta>
#if ( ! ${groups.isEmpty()} )
  <data>
    <groups>
#foreach ( $group in ${groups} )
      <element>${group}</element>
#end
    </groups>
  </data>
#else
  <data/>
#end
</ocs>